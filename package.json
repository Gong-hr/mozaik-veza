    "react": "^16.8.1",
    "react-day-picker": "^7.2.4",
    "react-dom": "^16.5.2",
    "react-ga": "^2.5.7",
    "react-paginate": "^5.3.1",
    "react-redux": "^5.1.1",
    "react-redux-loading-bar": "^4.2.0",
    "react-router-dom": "^4.3.1",
    "react-sanitized-html": "^2.0.0",
    "react-scripts": "^2.1.3",
    "react-vis": "^1.11.6",
    "react-window-size-listener": "^1.2.3",
